{
  "name": "reactathon",
  "version": "0.0.1",
  "description": "Hackathon starter kit with react, react-router, relay, and more.",
  "scripts": {
    "postinstall": "npm run build",
    "test": "npm run lint && npm run check && mocha $npm_package_options_mocha",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "eslint server/src",
    "lint:client": "eslint client/src",
    "check": "flow check",
    "build": "rm -rf lib/* && babel server/src --ignore __tests__ --out-dir lib && npm run copy",
    "copy": "cp -R server/src/views lib/views && cp -R server/src/database/config lib/database/config",
    "watch:tests": "babel tasks/watch.js | node",
    "cover": "babel-node node_modules/.bin/isparta cover --root src --report html node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "cover:lcov": "babel-node node_modules/.bin/isparta cover --root src --report lcovonly node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "start": "node --harmony ./lib/bin/server.js",
    "deploy:pm2": "npm test && npm run build && pm2 deploy ecosystem.json production",
    "stage:pm2": "npm test && npm run build && pm2 deploy ecosystem.json development",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "options": {
    "mocha": "--require tasks/mocha-bootload src/**/__tests__/**/*.js"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bcrypt": "^0.8.5",
    "body-parser": "~1.13.2",
    "browser-sync": "^2.10.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "chalk": "^1.1.1",
    "chalkline": "0.0.4",
    "chance": "^0.8.0",
    "compression": "^1.6.0",
    "connect-session-sequelize": "^3.0.0",
    "cookie-parser": "~1.3.5",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "express": "~4.13.1",
    "express-graphql": "^0.4.5",
    "express-phantom": "^0.1.2",
    "express-session": "^1.12.1",
    "faker": "^3.0.1",
    "figlet": "^1.1.1",
    "graphiql": "^0.4.5",
    "graphql": "^0.4.14",
    "graphql-relay": "^0.3.6",
    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.6",
    "hbs": "~3.1.0",
    "highlight.js": "^9.0.0",
    "isparta": "^4.0.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "morgan": "~1.6.1",
    "multiparty": "^4.1.2",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-local-sequelize": "^0.6.0",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "phantom": "^0.8.4",
    "pretty-error": "^2.0.0",
    "sane": "^1.3.0",
    "sequelize": "^3.15.1",
    "sequelize-cli": "^2.2.1",
    "sequelize-relay": "0.0.9",
    "sequelstore-connect": "^1.1.1",
    "serve-favicon": "~2.3.0",
    "session-file-store": "0.0.24",
    "session-passport-info": "^0.0.3",
    "sqlite3": "^3.1.1",
    "supertest": "^1.1.0",
    "winston": "^2.1.1"
  }
}
