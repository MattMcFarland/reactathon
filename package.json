    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.6",
    "hbs": "~3.1.0",
    "highlight.js": "^9.0.0",
    "history": "^1.17.0",
    "isparta": "^4.0.0",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "mocha": "^2.3.4",
    "moment": "^2.11.0",
    "morgan": "~1.6.1",
    "multiparty": "^4.1.2",
    "passport": "^0.3.2",
